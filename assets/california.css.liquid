@charset "UTF-8";
/* Liquid variables */
{% liquid
  assign color_body_bg = settings.color_body_bg
  assign color_body_text = settings.color_body_text
  assign color_accent = settings.color_accent
  assign color_section_bg = settings.color_section_bg
  assign color_alert = '#ff555d'
  assign color_notice = '#ffffcc'
  assign color_success = '#00730B'
  assign color_dark_slideshow = settings.color_dark_slideshow
  assign color_light_slideshow = settings.color_light_slideshow

  assign color_body_text_r = color_body_text | color_extract: 'red'
  assign color_body_text_g = color_body_text | color_extract: 'green'
  assign color_body_text_b = color_body_text | color_extract: 'blue'
  assign color_body_text_rgb = color_body_text_r | append: ', ' | append:  color_body_text_g | append: ', ' | append:  color_body_text_b

  assign color_body_text_light = color_body_text | color_modify: 'alpha', 0.6
  assign color_body_text_very_light = color_body_text | color_modify: 'alpha', 0.25
  assign color_body_text_dark = color_body_text | color_darken: 10
  assign color_body_text_lighten_5 = color_body_text | color_lighten: 5
  assign color_accent_dark = color_accent | color_darken: 5
  assign color_accent_light = color_accent | color_modify: 'alpha', 0.3

  assign color_hsl_array = settings.color_body_text | color_to_hsl | split: ', '
  assign color_lightness = color_hsl_array[2] | split: '%' | first | plus: 0 
  if color_lightness > 80
    assign control_color = '#1c1d1d'
  else
    assign control_color = '#fff'
  endif
  assign color_body_text_neg = control_color

  assign color_body_bg_darken_5 = color_body_bg | color_darken: 5

  assign color_hsl_array = settings.color_accent | color_to_hsl | split: ', '
  assign color_lightness = color_hsl_array[2] | split: '%' | first | plus: 0 
  if color_lightness > 80
    assign control_color = '#1c1d1d'
  else
    assign control_color = '#fff'
  endif
  assign color_accent_neg = control_color

  assign color_hsl_array = settings.color_alert | color_to_hsl | split: ', '
  assign color_lightness = color_hsl_array[2] | split: '%' | first | plus: 0 
  if color_lightness > 80
    assign control_color = '#1c1d1d'
  else
    assign control_color = '#fff'
  endif
  assign color_alert_neg = control_color

  assign color_side_nav_bg = settings.color_side_nav_bg
  assign color_side_nav_bg_darken_5 = color_side_nav_bg | color_darken: 5
  assign color_side_nav_text = settings.color_side_nav_text

  assign color_header_bg = settings.color_header_bg
  assign color_header_text = settings.color_header_text
  assign opacity_header_border = settings.opacity_header_border | divided_by: 100.0
  if opacity_header_border > 0
    assign color_header_border = settings.color_header_border | color_modify: 'alpha', opacity_header_border
    assign border_header = "1px solid " | append: color_header_border
  else
    assign color_header_border = "transparent"
    assign border_header = "none"
  endif
  assign color_header_bg_darken_5 = color_header_bg | color_darken: 5
  assign color_header_text_alpha_60 = color_header_text | color_modify: 'alpha', 0.60

  assign color_footer_bg = settings.color_footer_bg
  assign color_footer_title = settings.color_footer_title
  assign color_footer_text = settings.color_footer_text

  assign color_hsl_array = settings.color_footer_text | color_to_hsl | split: ', '
  assign color_lightness = color_hsl_array[2] | split: '%' | first | plus: 0 
  if color_lightness < 60
    assign highlight_color = color_footer_text | color_darken: 40
  else
    assign highlight_color = color_footer_text | color_lighten: 40
  endif
  assign color_footer_text_hl = highlight_color

  assign color_share_text = '#ffffff'
  assign color_facebook_bg = settings.color_facebook_bg
  assign color_twitter_bg = settings.color_twitter_bg
  assign color_pinterest_bg = settings.color_pinterest_bg

  assign content_max_width = settings.content_max_width
%}
/* Fonts */
{% liquid
  assign header_font = settings.header_font
  assign header_font_bold = header_font | font_modify: 'weight', 'bolder'
  assign header_weight_normal = header_font.weight | default: 400
  assign header_weight_bold = header_font_bold.weight | default: 700
  assign base_font = settings.base_font
  assign base_font_bolder = base_font | font_modify: 'weight', 'bolder'
  assign base_font_italic = base_font | font_modify: 'style', 'italic'
  assign base_font_bold_italic = base_font_bolder | font_modify: 'style', 'italic'
  assign base_font_bold = base_font | font_modify: 'style', 'bold'
  assign base_weight_normal = base_font.weight | default: 400
  assign base_weight_bolder = base_font_bolder.weight | default: 700
  assign base_weight_bold = base_font_bold.weight | default: 700
%}
