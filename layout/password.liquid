<!doctype html>
<!-- California Theme Version 9.0.1 -->
<html class="no-js" lang="{{ request.locale.iso_code }}">
  <head>
    {% render 'metas' %}
    {% render 'resources' %}
    {{ content_for_header }}
  </head>
  <body id="{{ page_title | handle }}" class="template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}" >
    <div class="main">
      <div class="layout">
        <div class="sections" id="Sections" role="main" >
          {{ content_for_layout }}
        </div>
      </div>
    </div>
  </body>
</html>